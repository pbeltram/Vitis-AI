{
    "name": "Xilinx GZIP Application For HBM Bandwidth", 
    "description": [
        "GZip application for HBM Bandwidth resides in `L3/benchmarks/gzip_hbm_bandwidth` directory."
    ], 
    "flow": "vitis", 
    "platform_blacklist": [
        "u200", 
        "u250", 
        "vck"
    ], 
    "platform_whitelist": [
        "u50", 
        "u280"
    ], 
    "launch": [
        {
            "cmd_args": "-sx BUILD/compress_decompress.xclbin -c PROJECT/sample.txt", 
            "name": "generic launch for all flows"
        }
    ], 
    "match_ini": "false", 
    "host": {
        "host_exe": "xil_gzip_8b", 
        "compiler": {
            "sources": [
                "./src/host.cpp", 
                "LIB_DIR/L3/src/zlib.cpp", 
                "LIB_DIR/common/libs/xcl2/xcl2.cpp", 
                "LIB_DIR/common/libs/cmdparser/cmdlineparser.cpp", 
                "LIB_DIR/common/libs/logger/logger.cpp",
                "LIB_DIR/common/thirdParty/zlib-1.2.7/adler32.c",
                "LIB_DIR/common/thirdParty/zlib-1.2.7/crc32.c"
            ], 
            "includepaths": [
                "LIB_DIR/L3/include", 
                "LIB_DIR/L1/include/hw",
                "LIB_DIR/common/thirdParty/zlib-1.2.7"
            ], 
            "options": "-O3 ", 
            "symbols": [
                "PARALLEL_BLOCK=4", 
                "C_COMPUTE_UNIT=2", 
                "D_COMPUTE_UNIT=9" 
            ]
        }
    }, 
    "v++": {
        "build_datafiles": [
            "PROJECT/advanced.cfg", 
            "PROJECT/hls_directives.tcl"
        ], 
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/tests/src/",
                "LIB_DIR/../security/L1/include"
            ], 
            "symbols": [
                "MULTIPLE_BYTES=8" 
            ], 
            "clflags": [
                "--config PROJECT/advanced.cfg"
            ]
        }
    }, 
    "containers": [
        {
            "name": "compress_decompress", 
            "ldclflags": "--config PROJECT/connectivity.cfg", 
            "accelerators": [
                {
                    "name": "xilGzipCompBlock", 
                    "location": "LIB_DIR/L2/src/gzip_compress_multicore_mm.cpp", 
                    "num_compute_units": "2", 
                    "compute_units": [
                        {
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "in", 
                                    "memory": "HBM[0:31]"
                                }, 
                                {
                                    "name": "out", 
                                    "memory": "HBM[0:31]"
                                }, 
                                {
                                    "name": "compressd_size", 
                                    "memory": "HBM[0:31]"
                                }, 
                                {
                                    "name": "checksumData", 
                                    "memory": "HBM[0:31]"
                                } 
                            ]
                        }, 
                        {
                            "slr": "SLR1", 
                            "arguments": [
                                {
                                    "name": "in", 
                                    "memory": "HBM[0:31]"
                                }, 
                                {
                                    "name": "out", 
                                    "memory": "HBM[0:31]"
                                }, 
                                {
                                    "name": "compressd_size", 
                                    "memory": "HBM[0:31]"
                                }, 
                                {
                                    "name": "checksumData", 
                                    "memory": "HBM[0:31]"
                                } 
                                
                            ]
                        }
                    ]
                }, 
                {
                    "name": "xilGzipS2MM", 
                    "location": "LIB_DIR/L2/tests/src/gzip_dm_s2mm.cpp", 
                    "num_compute_units": "9", 
                    "compute_units": [
                        {
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "out", 
